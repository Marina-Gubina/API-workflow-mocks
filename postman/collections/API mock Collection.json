					"event": [
						{
							"listen": "test",
							"script": {
								"id": "5da7bf60-525a-4974-aa5a-a06f828f90b2",
								"exec": [
									"let jsonData = pm.response.json();\r",
									"pm.environment.set(\"id_token\", jsonData.id_token)\r",
									"\r",
									"pm.test(\"Response status code is 201\", function () {\r",
									"    pm.expect(pm.response.code).to.eql(201);\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"Response contains required fields\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"\r",
									"    pm.expect(responseData).to.be.an('object');\r",
									"    pm.expect(responseData).to.have.property('id_token').that.is.a('string');\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"Id_token must be a non-empty string\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"\r",
									"    pm.expect(responseData).to.be.an('object');\r",
									"    pm.expect(responseData.id_token).to.exist.and.to.be.a('string').and.to.have.lengthOf.at.least(1, \"Id_token should not be empty\");\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"Response schema is valid\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"    \r",
									"    pm.expect(responseData).to.be.an('object');\r",
									"    pm.expect(responseData).to.have.all.keys('id_token');\r",
									"    pm.expect(responseData.id_token).to.be.a('string');\r",
									"});\r",
									"\r",
									""
								],
